{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust",
          "-webkit-tap-highlight-color",
          "text-decoration-skip-ink",
          "-webkit-overflow-scrolling",
          "orphans",
          "@page",
          "@page"
        ]
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "document-title": "off"
      }
    ],
    "button-type": "off",
    "meta-viewport": "off",
    "axe/language": "off",
    "meta-charset-utf-8": "off"
  }
}